	<queryString>
		<![CDATA[SELECT
nr.remi_id,
nr.remi_fecha_partida,
nr.remi_fecha_llegada,
nr.remi_motivo,
s.sucur_nom,
v.veh_mar||' - '||v.veh_mod||' - '||v.anho||' CHAPA '||v.vehi_cha as vehiculo,
cho.emp_nom||' '||cho.emp_ape as chofer,
cd.cantidad,
cd.monto,
m.mat_desc,
nr.remi_estado,
(cd.cantidad * cd.monto) as total
FROM nota_remision nr
JOIN sucursal s 
ON nr.sucur_id = s.sucur_id
JOIN vehiculo v
ON v.veh_id = nr.veh_id
JOIN empleado cho
ON cho.emp_id = nr.id_chofer
JOIN remision_compra rc
ON rc.remi_id = nr.remi_id
JOIN compra_detalle cd
ON cd.compra_id = rc.compra_id
JOIN materia_prima m 
ON m.cod_materia =  cd.cod_materia
WHERE nr.remi_fecha_partida BETWEEN  $P{desde} AND $P{hasta}]]>
	</queryString>
