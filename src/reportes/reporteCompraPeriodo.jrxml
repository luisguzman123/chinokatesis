	<parameter name="desde" class="java.util.Date"/>
	<parameter name="hasta" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
c.nro_factura,
c.tipo,
c.comp_fecha,
s.sucur_nom,
p.pro_nom,
SUM(cd.cantidad * cd.monto),
c.estado
FROM compra c
JOIN sucursal s
ON s.sucur_id = c.sucur_id
JOIN proveedor p 
ON p.pro_id =  c.pro_id
JOIN compra_detalle cd 
on cd.compra_id =  c.compra_id
WHERE c.comp_fecha BETWEEN $P{desde} AND $P{hasta}
GROUP BY c.nro_factura,
c.tipo,
c.comp_fecha,
s.sucur_nom,
p.pro_nom,
c.estado]]>
	</queryString>
