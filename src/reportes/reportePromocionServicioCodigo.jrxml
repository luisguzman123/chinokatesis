	<parameter name="codigo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
rc.id_promociones,
rc.fecha_ini,
rc.fecha_fin,
rc.beneficio ,
rc.promo_descri ,
rc.estado,
rc.observacion,
s.sucur_nom,
tt.descri_tipotrabajo,
rd.monto

FROM promociones_cabecera rc

JOIN promociones_detalle rd
ON rd.id_promociones =  rc.id_promociones


JOIN sucursal s 
ON s.sucur_id = rc.id_sucu 

JOIN tipos_trabajos tt
ON tt.id_tipo_trabajo = rd.id_tipo_trabajo

WHERE rc.id_promociones = $P{codigo}]]>
	</queryString>
