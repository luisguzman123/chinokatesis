	<parameter name="codigo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
rc.id_servi_termi_cab,
rc.fecha_entrega,
rc.descri,
rc.estado,
c.cli_nomb||' '||c.cli_apelli as cliente,
c.cli_ruc,
c.cli_telef,
rd.cantidad,
e.desc_equipo,
s.sucur_nom,
tt.descri_tipotrabajo,
rd.servi_termi_descri

FROM servicio_termi_cab rc

JOIN servicio_termi_det rd
ON rd.id_servi_termi_cab =  rc.id_servi_termi_cab

JOIN equipos e 
ON e.id_equipo =  rd.id_equipo


JOIN cliente c 
ON c.cli_cod = rc.id_cliente

JOIN sucursal s 
ON s.sucur_id = rc.id_sucursal

JOIN tipos_trabajos tt
ON tt.id_tipo_trabajo = rd.id_tipo_trabajo
WHERE rc.id_servi_termi_cab = $P{codigo}]]>
	</queryString>
