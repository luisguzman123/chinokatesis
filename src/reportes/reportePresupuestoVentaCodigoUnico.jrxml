	<queryString>
		<![CDATA[SELECT
pp.pre_cod,
pp.pre_estado,
pp.pre_fecha,
s.sucur_nom,
pd.cantidad,
pd.precio,
c.cli_nomb||' '||c.cli_apelli as cliente,
(pd.cantidad::INTEGER * pd.precio)as total,
mp.pro_desc

FROM presupuesto pp
JOIN  cliente c
ON c.cli_cod = pp.cli_cod
JOIN sucursal s 
ON s.sucur_id = pp.sucur_id
JOIN detalle_presupuesto pd
ON pd.pre_cod = pp.pre_cod
JOIN producto mp
ON mp.pro_cod = pd.pro_cod
WHERE pp.pre_cod = $P{codigo}]]>
	</queryString>
