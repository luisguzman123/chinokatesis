	<parameter name="codigo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
pc.cod_pedi_ven,
pc.fecha,
s.sucur_nom,
pc.estado,
mp.pro_desc ,
pd.cantidad,
c.cli_nomb||' '||c.cli_apelli as cliente
FROM pedido_venta pc
JOIN sucursal s 
ON s.sucur_id = pc.sucur_id
JOIN det_pedi_venta pd
ON pd.cod_pedi_ven = pc.cod_pedi_ven
JOIN producto mp
ON mp.pro_cod = pd.pro_cod
JOIN  cliente c
ON c.cli_cod = pc.cli_cod
WHERE pc.cod_pedi_ven = $P{codigo}]]>
	</queryString>
