	<parameter name="desde" class="java.lang.Integer"/>
	<parameter name="hasta" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
c.nro_factura,
c.ven_tipo,
c.ven_fecha,
s.sucur_nom,
p.cli_nomb ||' ' ||p.cli_apelli as cliente,
SUM(cd.det_cantidad * cd.de_monto),
c.estado
FROM ventas c
JOIN sucursal s
ON s.sucur_id = c.sucur_id
JOIN cliente p 
ON p.cli_cod =  c.cli_cod
JOIN detalle_venta cd 
on cd.ventas_id =  c.ventas_id
WHERE cd.ventas_id BETWEEN $P{desde}  AND $P{hasta}
GROUP BY c.nro_factura,
c.nro_factura,
c.ven_tipo,
c.ven_fecha,
s.sucur_nom,
p.cli_nomb ||' ' ||p.cli_apelli,
c.estado]]>
	</queryString>
