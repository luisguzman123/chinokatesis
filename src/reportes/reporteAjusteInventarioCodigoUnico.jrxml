	<queryString>
		<![CDATA[SELECT
a.ajustes_id,
a.ajus_fecha,
s.sucur_nom,
m.pro_desc,
da.cantidad,
a.estado
FROM ajustes a
JOIN sucursal s 
ON s.sucur_id = a.sucur_id
JOIN detalle_ajuste da 
ON da.ajustes_id = a.ajustes_id
JOIN producto m
ON m.pro_cod = da.pro_cod
WHERE a.ajustes_id  = $P{codigo}]]>
	</queryString>
