	<parameter name="codigo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
rc.fecha_registro,
rc.fecha_expiracion,
rc.observacion,
rc.estado,
c.cli_nomb||' '||c.cli_apelli as cliente,
e.desc_equipo,
tt.descripcion

FROM garantia_cab rc

JOIN garantia_det rd
ON rd.id_garantia_cab =  rc.id_garantia_cab

JOIN equipos e 
ON e.id_equipo =  rd.id_equipo


JOIN cliente c 
ON c.cli_cod = rc.id_cliente


JOIN tipos_garantia tt
ON tt.id_tipo_garantia = rd.id_tipo_garantia

WHERE rc.id_garantia_cab = $P{codigo}]]>
	</queryString>
