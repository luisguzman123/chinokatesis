	<parameter name="codigo" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
rc.id_descuentos,
rc.fecha,
rc.descuento_descri ,
rc.estado,
s.sucur_nom,
tt.descri_tipotrabajo,
rd.monto

FROM descuentos_cabecera rc

JOIN descuentos_detalle rd
ON rd.id_descuentos =  rc.id_descuentos


JOIN sucursal s 
ON s.sucur_id = rc.id_sucu 

JOIN tipos_trabajos tt
ON tt.id_tipo_trabajo = rd.id_tipo_trabajo

WHERE rc.id_descuentos = $P{codigo}]]>
	</queryString>
